  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "python:setup": "npm run python:create-env && npm run python:install",
    "python:create-env": "python -m venv python-env",
    "python:install": "npm run python:activate && pip install -r requirements.txt",
    "python:activate": "python-env\\Scripts\\activate",
    "python:test": "python python-scripts/test_clip_api.py --info",
    "python:clean": "rmdir /s /q python-env",
    "setup": "npm install && npm run python:setup",
    "test:python-api": "python python-scripts/test_clip_api.py"
  },
